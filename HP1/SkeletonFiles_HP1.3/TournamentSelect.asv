function selectedIndividualIndex = TournamentSelect(fitnessList, tournamentProbability, tournamentSize)


    selectedIndexes = randi([0, length(fitnessList)],1,tournamentSize)


    r = rand();

    fitnessList(selectedIndexes)
    % select highest fitness
    if (r < tournamentProbability)
        [M,I] = max(fitnessList(selectedIndexes))
        selectedIndividualIndex = selectedIndividualIndex


     

end
